{
    "name": "qa_tech_test",
    "version": "1.0.0",
    "scripts": {
        "precy:run": "rm -rf cypress/screenshots cypress/results cypress/reports",
        "cypress:open": "cypress open",
        "cypress:run": "cypress run",
        "report": "run-s report:*",
        "report:merge": "npx mochawesome-merge cypress/results/json/*.json -o cypress/results/mochawesome-bundle.json",
        "report:generate": "marge cypress/results/mochawesome-bundle.json -o cypress/reports/html",
        "test:report": "npm-run-all precy:run cypress:run report report:merge report:generate"
    },
    "author": "Jeremiah Hukpati",
    "license": "ISC",
    "devDependencies": {
        "cypress": "12.9.0",
        "cypress-multi-reporters": "1.6.3",
        "jest": "29.5.0",
        "mocha": "10.2.0",
        "mochawesome": "7.1.3",
        "mochawesome-merge": "4.3.0",
        "mochawesome-report-generator": "6.2.0",
        "npm-run-all": "4.1.5",
        "supertest": "6.3.3"
    }
}
